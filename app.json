{
  "expo": {
    "name": "whatsapp",
    "slug": "whatsapp",
    "version": "1.0.0",
    "scheme": "whatsapp",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/whatsapp.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/whatsapp.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manea5505.verta"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/whatsapp.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.manea5505.verta"
    }
  }
}
